    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#1859db",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#aa9f93"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#b5cea8"
                // "foreground": "#C9A0BE"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "support.function",
                // "entity.name.function"
            ],
            "settings": {
                "foreground": "#DCDCAA"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type"
            ],
            "settings": {
                "foreground": "#4ec9b0"
            }
        },
        {
            "name": "Variables / Parameters",
            "scope": [
                "variable",
                "text.variable"
            ],
            "settings": {
                "foreground": "#C9A0BE"
            }
        },
        {
            "name": "Core Keywords",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#61afef"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#A5B6C9"
            }
        }
    ]
}