    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#599a43",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#ad2a2a"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#a97213"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "support.function"
            ],
            "settings": {
                "foreground": "#9a10a1"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type"
            ],
            "settings": {
                "foreground": "#5B6C8A"
            }
        },
        {
            "name": "Variables / Parameters",
            "scope": [
                "variable",
                "text.variable"
            ],
            "settings": {
                "foreground": "#464646",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Core Keywords",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#0033CC"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#7A7A7A"
            }
        }
    ]
}