  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#3d4657",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#ffde98"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#9CFFED"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "support.function",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#f0efc0"
      }
    },
    {
      "name": "Types",
      "scope": [
        "storage.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#8BAED4"
      }
    },
    {
      "name": "Variables / Parameters",
      "scope": [
        "variable",
        "text.variable"
      ],
      "settings": {
        "foreground": "#C1CBD7",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Core Keywords",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#69BBFD"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#A5B6C9"
